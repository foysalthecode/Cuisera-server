model Feedback {
    id      String @id @default(uuid())
    content String @db.Text
    review  Int
    userId  String
    user    User   @relation(fields: [userId], references: [id])
    mealId  String
    meal    Meals  @relation(fields: [mealId], references: [id])

    @@index([userId])
    @@index([mealId])
}
