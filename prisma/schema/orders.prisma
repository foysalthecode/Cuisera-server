model Orders {
    id      String      @id @default(uuid())
    status  OrderStatus @default(PENDING)
    address String      @db.Text
    userId  String
    user    User        @relation(fields: [userId], references: [id])
    mealId  String
    meals   Meals       @relation(fields: [mealId], references: [id])
}

enum OrderStatus {
    CANCELED
    PENDING
    PREPARING
    READY
    DELIVERD
}
